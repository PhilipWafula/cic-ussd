
{
  "$schema": "https://json.schemastore.org/tsdoc.schema.json",
  "exclude": [
    "node_modules",
    "dist",
    "coverage",
    "docs",
    "test",
    "./src/lib/i18n/**/*",
    "./src/services/stateMachine/**/*"
  ],
  "excludePrivate": true,
  "theme": "default",
  "entryPointStrategy": "expand",
  "kindSortOrder": [
        "Reference",
        "Project",
        "Module",
        "Namespace",
        "Enum",
        "EnumMember",
        "Class",
        "Interface",
        "TypeAlias",
        "Constructor",
        "Property",
        "Variable",
        "Function",
        "Accessor",
        "Method",
        "ObjectLiteral",
        "Parameter",
        "TypeParameter",
        "TypeLiteral",
        "CallSignature",
        "ConstructorSignature",
        "IndexSignature",
        "GetSignature",
        "SetSignature"
    ],
  "excludeNotDocumented": true,
  "excludeExternals": true
}